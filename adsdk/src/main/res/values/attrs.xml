<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MyAdView">
        <attr name="setTopBorder" format="boolean" />
        <attr name="setBottomBorder" format="boolean" />
    </declare-styleable>

    <declare-styleable name="NativeAdView">
        <attr name="gnt_template_type" format="reference" />
    </declare-styleable>
    <declare-styleable name="CreateNativeAds">
        <attr name="native_ad_id" format="string"/>
        <attr name="native_ad_height" format="dimension"/>
        <attr name="buildType" format="string"/>
        <attr name="setTitleTextColor" format="color"/>
        <attr name="setBodyTextColor" format="color"/>
        <attr name="setAdTextColor" format="color"/>
        <attr name="setActionButtonTextColor" format="reference|color"/>
        <attr name="setActionButtonBackground" format="reference"/>
        <attr name="setAdTextBackground" format="reference"/>
    </declare-styleable>
</resources>